{
    "name": "OpenVPN",
    "version": "0.2.2",
    "slug": "openvpn",
    "description": "OpenVPN Bridging",
    "url": "https://github.com/greylurk/hassio-addon-experiments",
    "startup": "application",
    "boot": "auto",
    "arch": [
    "amd64",
    "armhf",
    "i386"
  ],
    "options": {
        "ovpn_file": ""
    },
    "schema": {
        "ovpn_file": "str"
    },
    "map": [ "config" ],
    "host_network": true,
    "devices": [ "/dev/net/tun:/dev/net/tun:rw" ],
    "privileged": [ "NET_ADMIN" ]
}
